{
  "name": "projecto-adb",
  "version": "1.0.0",
  "description": "Electron app build using ReactJS and Parcel web application bundler",
  "main": "src/electron.js",
  "scripts": {
    "react-start": "parcel -p 3000 index.html --out-dir build",
    "react-build": "parcel build index.html --out-dir build --public-url ./",
    "electron-build": "electron-builder -mwl",
    "clean-build": "rm -rf build/ .cache dist/",
    "build": "npm run clean-build && npm run react-build && npm run electron-build",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron . \""
  },
  "keywords": [
    "electron",
    "react",
    "parcel",
    "electron-react-parcel",
    "electron react parcel"
  ],
  "author": "Equipo 3",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@babel/runtime-corejs2": "^7.7.4",
    "electron-is-dev": "^1.1.0",
    "electron-store": "^5.1.0",
    "img": "^3.0.3",
    "mssql": "^6.0.1",
    "nav.css": "^1.0.0",
    "react": "^16.12.0",
    "react-awesome-modal": "^2.0.5",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "reactjs-popup": "^1.5.0",
    "reactstrap": "^8.2.0",
    "semantic-ui-css": "^2.4.1"
  },
  "build": {
    "appId": "com.projectoadb",
    "files": [
      "src/electron.js",
      "src/assets/*",
      "build/**/*"
    ],
    "mac": {
      "target": "dmg",
      "icon": "src/assets/logo.png"
    },
    "win": {
      "target": "nsis",
      "icon": "src/assets/logo.png"
    },
    "linux": {
      "target": "deb",
      "icon": "src/assets/logo.png",
      "category": "Development"
    }
  },
  "homepage": "./",
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-transform-regenerator": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "babel": "^6.23.0",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "electron": "^13.6.6",
    "electron-builder": "^21.2.0",
    "parcel-bundler": "^1.12.4",
    "wait-on": "^3.3.0"
  }
}
